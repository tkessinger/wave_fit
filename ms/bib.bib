	Issn = {0022-5193},
	Journal = {J Theor Biol},
	Keywords = {Bellman–Harris branching process, Stochastic evolution},
	Month = may,
	Pages = {65--73},
	Title = {Fixation times in differentiation and evolution in the presence of bottlenecks, deserts, and oases},
	Url = {http://www.sciencedirect.com/science/article/pii/S0022519315000922},
	Volume = {372},

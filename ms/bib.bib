	Keywords = {CXCR4, effective population size, fitness epistasis, fitness valley, human immunodeficiency virus type 1, stochastic tunnelling},
	Month = feb,
	Number = {3},
	Pages = {604--615},
	Title = {Fitness valleys constrain HIV‐1's adaptation to its secondary chemokine coreceptor},
	Url = {https://onlinelibrary-wiley-com.proxy.lib.duke.edu/doi/full/10.1111/jeb.12329},
	Volume = {27},

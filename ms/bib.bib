	Issn = {0737-4038},
	Journal = {Mol Biol Evol},
	Language = {en},
	Month = sep,
	Number = {9},
	Pages = {2454--2468},
	Title = {How Good Are Statistical Models at Approximating Complex Fitness Landscapes?},
	Url = {https://academic-oup-com.proxy.lib.duke.edu/mbe/article/33/9/2454/2578922},
	Volume = {33},

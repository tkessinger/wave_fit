	Language = {en},
	Month = dec,
	Number = {12},
	Pages = {e1005066},
	Title = {Different Evolutionary Paths to Complexity for Small and Large Populations of Digital Organisms},
	Url = {http://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1005066},
	Volume = {12},

	Journal = {J Theor Biol},
	Keywords = {Adaptation, Asexual population, Extreme value theory, Genotype space},
	Month = may,
	Pages = {89--102},
	Title = {Greedy adaptive walks on a correlated fitness landscape},
	Url = {http://www.sciencedirect.com/science/article/pii/S0022519316001375},
	Volume = {397},

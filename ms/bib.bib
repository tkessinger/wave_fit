@article{ochs_2015,
	Abstract = {On rugged fitness landscapes where sign epistasis is common, adaptation can often involve either individually beneficial ``uphill'' mutations or more complex mutational trajectories involving fitness valleys or plateaus. The dynamics of the evolutionary process determine the probability that evolution will take any specific path among a variety of competing possible trajectories. Understanding this evolutionary choice is essential if we are to understand the outcomes and predictability of adaptation on rugged landscapes.},
	Author = {Ochs, Ian E. and Desai, Michael M.},
	Day = {26},
	Doi = {10.1186/s12862-015-0334-0},

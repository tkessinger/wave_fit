	Keywords = {DNA recombination, Entropy, Epistasis, Fitness epistasis, Natural selection, Population dynamics, Population size, Sign epistasis},
	Month = sep,
	Number = {9},
	Pages = {e1003836},
	Title = {Multidimensional Epistasis and the Transitory Advantage of Sex},
	Url = {http://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1003836},
	Volume = {10},
	Year = {2014},
	Bdsk-File-1 = {},

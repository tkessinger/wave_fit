	Shorttitle = {Adaptation in {Tunably} {Rugged} {Fitness} {Landscapes}},
	Title = {Adaptation in Tunably Rugged Fitness Landscapes: The Rough Mount Fuji Model},
	Url = {http://www.genetics.org/content/198/2/699},

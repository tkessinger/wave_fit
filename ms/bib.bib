	Number = {8},
	Pages = {e1002134},
	Publisher = {Public Library of Science},
	Title = {Evolutionary Accessibility of Mutational Pathways},
	Url = {http://dx.doi.org/10.1371%2Fjournal.pcbi.1002134},

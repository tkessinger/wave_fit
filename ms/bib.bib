	Num-Scopus-Cites = {16},
	Number = {1727},
	Pages = {247-256},
	Title = {Impact of epistasis and pleiotropy on evolutionary adaptation},
	Url = {http://rspb.royalsocietypublishing.org/content/279/1727/247.abstract},
	Volume = {279},

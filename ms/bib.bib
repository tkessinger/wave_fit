	Keywords = {Evolutionary biology, Evolutionary genetics, Fitness epistasis, Nucleotide sequencing, Organismal evolution, Point mutation, Repeated sequences, Variant genotypes},
	Month = dec,
	Number = {12},
	Pages = {e1005218},
	Shorttitle = {Beyond the {Hypercube}},
	Title = {Beyond the Hypercube: Evolutionary Accessibility of Fitness Landscapes with Realistic Mutational Networks},
	Url = {http://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1005218},
	Volume = {12},
	Year = {2016},

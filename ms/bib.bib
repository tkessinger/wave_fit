	Month = jan,
	Note = {{ArticleType:} research-article / Full publication date: 1995 / Copyright © 1995 Annual Reviews},
	Num-Scopus-Cites = {240},
	Pages = {601--629},
	Title = {Multiple Fitness Peaks and Epistasis},
	Url = {http://www.jstor.org/stable/2097221},

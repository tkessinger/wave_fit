	Shorttitle = {Population {Size} {Affects} {Adaptation} in {Complex} {Ways}},
	Title = {Population Size Affects Adaptation in Complex Ways: Simulations on Empirical Adaptive Landscapes},
	Url = {http://link.springer.com/article/10.1007/s11692-017-9440-9},
